filebeat:
  prospectors:
    -
      paths:
        - /var/log/nginx.log
      input_type: log
      document_type: nginx_server2
    -
      paths:
        - /var/log/uwsgi.log
      input_type: log
      document_type: wsgi_server2
  registry: /var/lib/filebeat/registry
output:
  logstash:
    hosts: ["<IP>:5044"]
logging:
  to_files: true
  files:
    path: /var/log/filebeat
    name: filebeat
    rotateeverybytes: 10485760
  level: error